<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBlogServlet</servlet-name>
        <servlet-class>controller.marketer.AddBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBlogServlet</servlet-name>
        <servlet-class>controller.marketer.DeleteBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBlogServlet</servlet-name>
        <servlet-class>controller.marketer.EditBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListManageBlogServlet</servlet-name>
        <servlet-class>controller.marketer.ListManageBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListManageCustomerServlet</servlet-name>
        <servlet-class>controller.marketer.ListManageCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListManageFeedback</servlet-name>
        <servlet-class>controller.marketer.ListManageFeedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBlogServlet</servlet-name>
        <url-pattern>/addblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBlogServlet</servlet-name>
        <url-pattern>/deleteblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBlogServlet</servlet-name>
        <url-pattern>/editblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListManageBlogServlet</servlet-name>
        <url-pattern>/listmanageblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListManageCustomerServlet</servlet-name>
        <url-pattern>/listmanagecustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListManageFeedback</servlet-name>
        <url-pattern>/listmanagefeedback</url-pattern>
    </servlet-mapping>
</web-app>
