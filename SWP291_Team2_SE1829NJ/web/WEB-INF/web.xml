<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>controller.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>controller.admin.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>roomManagement</servlet-name>
        <servlet-class>controller.admin.roomManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>accountManagement</servlet-name>
        <servlet-class>controller.admin.accountManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListManageBlogServlet</servlet-name>
        <servlet-class>controller.marketer.ListManageBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBlogServlet</servlet-name>
        <servlet-class>controller.marketer.AddBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBlogServlet</servlet-name>
        <servlet-class>controller.marketer.EditBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBlogServlet</servlet-name>
        <servlet-class>controller.marketer.DeleteBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCustomerServlet</servlet-name>
        <servlet-class>controller.marketer.ViewCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListFeedbackServlet</servlet-name>
        <servlet-class>controller.marketer.ListFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>roomManagement</servlet-name>
        <url-pattern>/roommanagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>accountManagement</servlet-name>
        <url-pattern>/accountmanagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListManageBlogServlet</servlet-name>
        <url-pattern>/listmanageblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBlogServlet</servlet-name>
        <url-pattern>/addblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBlogServlet</servlet-name>
        <url-pattern>/editblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBlogServlet</servlet-name>
        <url-pattern>/deleteblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCustomerServlet</servlet-name>
        <url-pattern>/viewcustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListFeedbackServlet</servlet-name>
        <url-pattern>/listfeedback</url-pattern>
    </servlet-mapping>
</web-app>
